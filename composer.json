{
    "name": "vitnasinec/image",
    "description": "Just register and configure league/glide",
    "license": "MIT",
    "authors": [
        {
            "name": "Vit Nasinec",
            "email": "vitnasinec@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^9.0|^10.0",
        "league/glide": "^2.0",
        "league/glide-symfony": "^2.0"
    },
    "autoload": {
        "psr-4": {"VitNasinec\\Image\\": "src/"}
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "VitNasinec\\Image\\ImageServiceProvider"
            ]
        }
    }
}
